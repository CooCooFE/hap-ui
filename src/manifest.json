{
  "package": "com.application.demo",
  "name": "quickapp-ui",
  "versionName": "1.0.0",
  "versionCode": "1",
  "minPlatformVersion": "101",
  "icon": "/Common/logo.png",
  "features": [
    { "name": "system.prompt" },
    { "name": "system.router" },
    { "name": "system.shortcut" },
    { "name": "system.share" },
    { "name": "system.webview" }
  ],
  "permissions": [
    { "origin": "*" }
  ],
  "config": {
    "logLevel": "debug"
  },
  "router": {
    "entry": "Home",
    "pages": {
      "Home": {
        "component": "index"
      },
      "components/ux-button": {
        "component": "index"
      },
      "components/ux-titleBar": {
        "component": "index"
      },
      "components/ux-lightbox": {
        "component": "index"
      },
      "components/ux-searchbar": {
        "component": "index"
      },
      "components/ux-tabBar": {
        "component": "index"
      },
      "components/ux-checkbox": {
        "component": "index"
      },
      "components/ux-toast": {
        "component": "index"
      }
    }
  },
  "display": {
    "titleBarBackgroundColor": "#f2f2f2",
    "titleBarTextColor": "#414141",
    "menu": true,
    "pages": {
      "Home": {
        "titleBarText": "quickApp-UI",
        "titleBar": false,
        "menu": false
      }
    }
  }
}